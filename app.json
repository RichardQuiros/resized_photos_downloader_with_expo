{
  "expo": {
    "name": "Resized photos downloader",
    "slug": "RPD",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nekocode.RPD"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "dark-content",
      "backgroundColor": "#1a1a237e"
    },
    "android": {
      "package": "com.nekoapp.resized_photos_downloader",
      "versionCode": 1
    },

    "description": "This is an application that shows images with the size of your device and the user can download this image on. a smartphone or browser",
    "githubUrl": "https://github.com/RichardQuiros/resized_photos_downloader_with_expo.git"
  }
}
